#include <iostream>

using namespace std;


void multiplicar();

int main() {
    multiplicar();
    return 0;
}

void multiplicar() {
    int x, dobro;

    
    while (true) {
        cin >> x;

        
        if (x == 0) {
            break;
        }

        dobro = x * 2;
        cout << "O dobro do numero e: " << dobro << endl;
    }
}
