#include <iostream>

using namespace std;

void inte()
{
    float k;
    cin>>k;
    if(k>0){
        cout<<"1";
    }else{
        if(k<0){
            cout<<"-1";
        }else{
            if(k==0){
                cout<<"0";
            }
        }
    }
}

int main()
{
    inte();

    return 0;
}
